library("CaTools")
library("psych")
library("caret")
Student_Scores_and_Hours <- read.csv("https://raw.githubusercontent.com/AdiPersonalWorks/Random/master/student_scores%20-%20student_scores.csv", Hours = unlist(Hours))
head(Student_Scores_and_Hours)
tail(Student_Scores_and_Hours)
summary(Student_Scores_and_Hours)
str(Student_Scores_and_Hours)
which(is.na(Student_Scores_and_Hours))
pairs.panels(Student_Scores_and_Hours[c("Hours", "Scores")])
boxplot(Student_Scores_and_Hours$Hours)
boxplot(Student_Scores_and_Hours$Scores)
set.seed(0)
split= sample.split(Student_Scores_and_Hours, SplitRatio=0.8)
training_set = subset(Student_Scores_and_Hours,split==TRUE)
test_set = subset(Student_Scores_and_Hours,split==FALSE)
Model <- train(Scores~Hours ,data=training_set, method ="lm")
train_a = predict(lm_a,training_set)
test_a = predict(lm_a,test_set)
summary(Model)
plot(training_set$Scores, train_a ,col = "blue")
plot(test_set$Scores, test_a ,col = "blue" )
cor(test_set$Scores, test_a)
predict(Model, list(Hours = 9.25))
rmse(test_set$Scores, test_a)
rmse(training_set$Scores, train_a )
